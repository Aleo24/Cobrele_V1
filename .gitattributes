import turtle 

t = turtle.Turtle()
t.color("white")
t.goto(-80,-60)
t.speed(20)
t.color("red")
def curveleft(limita,finete,distanta):
    for i in range(limita):
     t.left(finete)
     t.forward(distanta)

def curveright(limita,finete,distanta):
    for i in range(limita):
     t.right(finete)
     t.forward(distanta)

def dreptunghi1():
   t.color("midnightblue")
   t.begin_fill()
   t.left(30)
   t.forward(50)
   t.right(120)
   t.forward(25)
   t.left(90)
   t.forward(100)
   print(t.pos()) 
   t.left(90)
   t.forward(60)
   t.left(90)
   t.forward(100)
   t.left(90)
   t.forward(35)
   t.end_fill()
   t.begin_fill()
   t.goto(-80,-60)
   t.left(130)
   t.forward(60)
   t.end_fill()
def gura():
   t.penup()
   t.goto(-30,-30)
   t.pendown()
   t.color("red")
   t.begin_fill()
   t.forward(20)
   curveright(20,5,1.5)
   t.left(120)
   curveright(20,5,1.5)
   t.forward(20)
   t.right(110)
   t.forward(35)
   curveright(10,6.2,2.5)
   t.forward(34)
   t.end_fill()
   t.color("black")
   t.penup()
   t.right(150)
   t.forward(5)
   t.pendown()
dreptunghi1()
gura()
def talloval(k):              
    t.left(45)
    for i in range(2):     
        t.circle(k,90)    
        t.circle(k/2,90)  

def flatoval(k):              
    t.right(45)
    for i in range(2):
        t.circle(k,90)
        t.circle(k/2,90)



#outline ureche
t.penup()
t.goto(-25,-30)
t.pendown()
t.color("light sea green")
t.speed(10)
t.begin_fill()
t.left(130)
t.forward(80)
print(t.pos())
t.backward(80)
t.right(10)
t.forward(80)
print(t.pos())
t.goto(-74.2529180261, 33.0408602885)
t.end_fill()
t.goto(-62.5577250229, 40.6358074287)
t.right(30)
t.begin_fill()
flatoval(70)
t.end_fill()
print(t.pos())


#outline + ochi
t.penup()
t.goto(-85,60)
t.pendown()
t.begin_fill()
t.color("orange red")
t.forward(70)
t.left(45)
t.forward(80)
t.right(90)
t.forward(70)
t.right(90)
t.forward(80)
t.right(90)
t.forward(40)
print(t.pos())
t.left(32)
t.forward(92)
t.end_fill()

def ureche():
   t.color("gold")
   t.penup()
   t.goto(-60,60)
   t.pendown()
   t.begin_fill()
   t.right(120)
   t.forward(60)
   curveleft(43,5,2)
   t.right(35)
   t.forward(30)
   curveleft(30,5,2)
   t.end_fill()
   t.color("black")
   t.penup()
   t.goto(-70,100)
   t.pendown()
   t.pensize(5)
   t.left(30)
   t.forward(20)
   curveleft(40,6,1.5)
   t.right(60)
   t.forward(20)
   curveleft(20,7.7,0.9)
   t.forward(18)
   
ureche()   

t.color("white")
t.penup()
t.goto(-20,140)
t.pendown()
t.right(60)
t.begin_fill()
flatoval(30)
t.end_fill()



#ochi si spranceana
t.color("black")
t.penup()
t.goto(0,150)
t.dot(40)
t.goto(-28,170)
t.pendown()
t.pensize(5)
t.left(45)
t.left(30)
l=18
curveright(18,3,3.5)



#mana
t.penup()
t.goto(63,-60)
t.pendown()
t.color("burlywood")
t.pensize(1)
t.begin_fill()
t.left(20)
t.forward(10)
t.backward(10)
t.left(60)
t.forward(20)
t.right(130)
t.forward(20)
t.end_fill()
t.begin_fill()
curveleft(360,1,1)
t.end_fill()
t.penup()
t.goto(100,-85)   #de unde totul a inceput 
t.pendown()
t.fillcolor("gold")
t.begin_fill()
t.pencolor("goldenrod")
t.left(160)
t.forward(50)
for i in range(3):
   curveright(20,9,1.1)
   t.forward(20)
   t.left(180)
   t.forward(30)
curveright(20,9,1.1)
t.forward(50)
t.left(180)
t.forward(20)
curveright(20,9,1.1)
t.forward(40)
print(t.pos()) # unde totul s a terminat
t.end_fill()



#nasul si outline-ul
t.penup()
t.pensize(1)
t.color("orange")
t.goto(-35,55)
t.begin_fill()
t.circle(55)
t.end_fill()
t.goto(20,100) 
t.pendown()
t.color("gold")
t.begin_fill()
t.left(85)
t.forward(20)
t.right(90)
t.left(10)
curveleft(20,1.5,3)
curveright(10,10,3)
curveright(6,16,3.5)
print(t.pos()) #nara 1
t.penup()
t.goto(20,100)
t.pendown()
t.left(150)
curveright(20,1.1,3)
t.right(10)
curveleft(10,10,3)
curveleft(6,16,3.5)
print(t.pos()) #nara 2
t.right(130)
curveleft(15,0.6,0.6)
t.left(30)
curveleft(10,1.2,0.6)
t.left(50)
curveleft(9,1.3,0.8)
t.goto(35.9019261715, 27.6335462786)
t.end_fill()
