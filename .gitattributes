import turtle 

t = turtle.Turtle()
t.color("white")
t.goto(-80,-60)
t.speed(20)
t.color("red")
def curveleft(finete,distanta):
    for i in range(l):
     t.left(finete)
     t.forward(distanta)
def curveright(f,d):
    for i in range(l):
     t.right(f)
     t.forward(d)
def curve1(k):
    for i in range(k):
        t.right(5)
        t.forward(1.5)
def curve2(k):
    for i in range(k):
        t.right(6.2)
        t.forward(2.5)  
def curve3(k):
    for i in range(k):
        t.right(8)
        t.forward(4)
def curve4(k):
   for i in range(k):
        t.left(5)
        t.forward(2)
def curve5(k):
   for i in range(k):
        t.left(6)
        t.forward(1.5)
def curve6(k):
   for i in range(k):
        t.left(7.7)
        t.forward(0.9)
def curve7(k):
   for i in range(k):
        t.left(1)
        t.forward(1)
def curve8(k):
   for i in range(k):
        t.right(9)
        t.forward(1.1)
def dreptunghi1():
   t.color("blue")
   t.begin_fill()
   t.left(30)
   t.forward(50)
   t.right(120)
   t.forward(25)
   t.left(90)
   t.forward(100)
   print(t.pos()) #coord(mana)
   t.left(90)
   t.forward(60)
   t.left(90)
   t.forward(100)
   t.left(90)
   t.forward(35)
   t.end_fill()
   t.begin_fill()
   t.goto(-80,-60)
   t.left(130)
   t.forward(60)
   t.end_fill()
def gura():
   t.penup()
   t.goto(-30,-30)
   t.pendown()
   t.color("red")
   t.begin_fill()
   t.forward(20)
   curve1(20)
   t.left(120)
   curve1(20)
   t.forward(20)
   t.right(110)
   t.forward(35)
   curve2(10)
   t.forward(34)
   t.end_fill()
   t.color("black")
   t.penup()
   t.right(150)
   t.forward(5)
   t.pendown()
dreptunghi1()
gura()
def talloval(k):              
    t.left(45)
    for i in range(2):     
        t.circle(k,90)    
        t.circle(k/2,90)  

def flatoval(k):              
    t.right(45)
    for i in range(2):
        t.circle(k,90)
        t.circle(k/2,90)
#outline ureche
t.penup()
t.goto(-25,-30)
t.pendown()
t.color("cyan")
t.speed(10)
t.begin_fill()
t.left(130)
t.forward(80)
t.backward(80)
t.right(10)
t.forward(80)
t.right(30)

t.end_fill()

t.begin_fill()
flatoval(70)
t.end_fill()
print(t.pos())
