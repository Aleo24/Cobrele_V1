# Auto detect text files and perform LF normalization
* text=auto
import turtle 

t = turtle.Turtle()
t.color("white")
t.goto(-80,-60)
t.speed(20)
t.color("red")

def curve1(k):
    for i in range(k):
        t.right(5)
        t.forward(1.5)
def curve2(k):
    for i in range(k):
        t.right(6.2)
        t.forward(2.5)  
def curve3(k):
    for i in range(k):
        t.right(8)
        t.forward(4)
def dreptunghi1():
   t.color("blue")
   t.begin_fill()
   t.left(30)
   t.forward(50)
   t.right(120)
   t.forward(25)
   t.left(90)
   t.forward(100)
   print(t.pos()) #coord(mana)
   t.left(90)
   t.forward(60)
   t.left(90)
   t.forward(100)
   t.left(90)
   t.forward(35)
   t.end_fill()
   t.begin_fill()
   t.goto(-80,-60)
   t.left(130)
   t.forward(60)
   t.end_fill()
def gura():
   t.penup()
   t.goto(-30,-30)
   t.pendown()
   t.color("red")
   t.begin_fill()
   t.forward(20)
   curve1(20)
   t.left(120)
   curve1(20)
   t.forward(20)
   t.right(110)
   t.forward(35)
   curve2(10)
   t.forward(34)
   t.end_fill()
   t.color("black")
   t.penup()
   t.right(150)
   t.forward(5)
   t.pendown()
dreptunghi1()
gura()
def talloval(k):              
    t.left(45)
    for i in range(2):     
        t.circle(k,90)    
        t.circle(k/2,90)  

def flatoval(k):              
    t.right(45)
    for i in range(2):
        t.circle(k,90)
        t.circle(k/2,90)
#outline ureche
t.penup()
t.goto(-25,-30)
t.pendown()
t.color("cyan")
t.speed(10)
t.left(130)
t.forward(80)
t.backward(80)
t.right(10)
t.forward(80)
t.right(30)
t.begin_fill()
flatoval(70)
t.end_fill()
print(t.pos())
#outline + ochi
t.penup()
t.goto(-85,60)
t.pendown()
t.begin_fill()
t.color("red")
t.forward(70)
t.left(45)
t.forward(80)
t.right(90)
t.forward(70)
t.right(90)
t.forward(80)
t.right(90)
t.forward(40)
print(t.pos())
t.left(32)
t.forward(92)
t.end_fill()




