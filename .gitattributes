t.end_fill()

def ureche():
   t.color("gold")
   t.penup()
   t.goto(-60,60)
   t.pendown()
   t.begin_fill()
   t.right(120)
   t.forward(60)
   curve4(43)
   t.right(35)
   t.forward(30)
   curve4(30)
   t.end_fill()
   t.color("black")
   t.penup()
   t.goto(-70,100)
   t.pendown()
   t.pensize(5)
   t.left(30)
   t.forward(20)
   curve5(40)
   t.right(60)
   t.forward(20)
   curve6(20)
   t.forward(18)
   
ureche()   

